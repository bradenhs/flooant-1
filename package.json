    "mobx": "^3.5.1",
    "mobx-react": "^4.4.2",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-native": "0.52.0",
    "react-native-animatable": "^1.2.4",
    "react-native-scripts": "^1.11.1",
