{
  "name": "flooant",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start":
      "source .env && stmux -t \"Flooant Build Environment\" -w always -M -f stmux.config",
    "typechecker": "tsc --project tsconfig.json --pretty --noEmit --watch",
    "server":
      "run.env ts-node-dev -r tsconfig-paths/register ./server/main/index.ts",
    "clean": "source .env && rm -rf $DATA_DIR",
    "client": "react-native-scripts start",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "db":
      "source .env && mkdir -p $DATA_DIR && gcloud beta emulators datastore start --project $DATASTORE_PROJECT_ID --no-store-on-disk --host-port $DATASTORE_EMULATOR_HOST --data-dir $DATA_DIR"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.3.3",
    "@koa/cors": "^2.2.1",
    "expo": "^25.0.0",
    "jsonwebtoken": "^8.1.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "react": "^16.2.0",
    "react-native": "0.52.0",
    "serialize-error": "^2.1.0",
    "tslib": "^1.9.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/google-cloud__datastore": "^1.3.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/koa": "^2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.27",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.12",
    "@types/winston": "^2.3.7",
    "prettier": "^1.10.2",
    "react-native-scripts": "^1.11.1",
    "react-native-typescript-transformer": "^1.2.3",
    "run.env": "^1.0.1",
    "stmux": "^1.4.19",
    "ts-node": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.9",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0-dev.20180209"
  }
}
