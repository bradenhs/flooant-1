{
  "name": "flooant",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "typecheck": "tsc --project tsconfig.json --noEmit --pretty --watch",
    "web-app":
      "source .env && parcel serve ./web-app/index.html -p $WEB_APP_PORT",
    "server":
      "run.env ts-node-dev -r tsconfig-paths/register ./server/main/index.ts",
    "clean": "source .env && rm -rf $DATA_DIR",
    "db":
      "source .env && mkdir -p $DATA_DIR && gcloud beta emulators datastore start --project $DATASTORE_PROJECT_ID --host-port $DATASTORE_EMULATOR_HOST --data-dir $DATA_DIR"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.3.3",
    "@koa/cors": "^2.2.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "serialize-error": "^2.1.0",
    "tslib": "^1.9.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/google-cloud__datastore": "^1.3.0",
    "@types/koa": "^2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.27",
    "@types/winston": "^2.3.7",
    "parcel-bundler": "^1.5.1",
    "prettier": "^1.10.2",
    "run.env": "^1.0.1",
    "ts-node": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.9",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0-dev.20180209"
  }
}
